{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "ESNext",
    "target": "ES2022",
    "jsx": "react-jsx",
    "resolveJsonModule": true,
    "allowJs": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "App": ["src/App"]
    },
    "allowSyntheticDefaultImports": true,
    "allowUmdGlobalAccess": true
  }
}
